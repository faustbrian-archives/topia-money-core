{
    "name": "kodekeep/topia-money",
    "description": "A strongly defined specification of common interactions with Cryptocurrency Data Services.",
    "keywords": [
        "kodekeep",
        "topia-money"
    ],
    "homepage": "https://github.com/kodekeep/topia-money",
    "license": "MPL-2.0",
    "authors": [{
        "name": "Brian Faust",
        "email": "brian@kodekeep.com",
        "homepage": "https://kodekeep.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.4",
        "graham-campbell/guzzle-factory": "^4.0",
        "illuminate/contracts": "^8.0",
        "illuminate/http": "^8.5",
        "illuminate/support": "^8.0",
        "nesbot/carbon": "^2.40",
        "paquettg/php-html-parser": "^3.1",
        "spatie/data-transfer-object": "^2.5",
        "spatie/enum": "^3.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "graham-campbell/analyzer": "^3.0",
        "orchestra/testbench": "^6.0",
        "pestphp/pest": "^0.3.5",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "KodeKeep\\TopiaMoney\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KodeKeep\\TopiaMoney\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "./vendor/bin/pest --coverage --coverage-html=.coverage --coverage-clover=coverage.xml",
        "format": "vendor/bin/php-cs-fixer fix ./",
        "analyse": "vendor/bin/phpstan analyse --memory-limit=2G"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

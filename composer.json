{
    "name": "kodekeep/:package_name",
    "description": ":package_description",
    "keywords": [
        "kodekeep",
        ":package_name"
    ],
    "homepage": "https://github.com/kodekeep/:package_name",
    "license": "MPL-2.0",
    "authors": [{
        "name": "Brian Faust",
        "email": "brian@kodekeep.com",
        "homepage": "https://kodekeep.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.4",
        "illuminate/contracts": "^8.0",
        "illuminate/http": "^8.5",
        "illuminate/support": "^8.0",
        "nesbot/carbon": "^2.40",
        "spatie/data-transfer-object": "^2.5",
        "spatie/enum": "^3.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "graham-campbell/analyzer": "^3.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "KodeKeep\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KodeKeep\\Skeleton\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html=.coverage --coverage-clover=clover.xml",
        "format": "vendor/bin/php-cs-fixer fix ./",
        "analyse": "vendor/bin/phpstan analyse --memory-limit=2G"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
